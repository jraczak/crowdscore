<%= render partial: 'admin/subnav' %>

<%= form_for resource, :url => {:controller => "featured_lists", :action => "create"} do |f| %>

  <%= f.label "List ID" %><br>
  <%= f.text_field :list_id %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :city %><br>
  <%= f.text_field :city %><br>
  <%= f.label :state %><br>
  <%= f.text_field :state %><br>
  <%= f.label :name, "Name (optional)" %><br>
  <%= f.text_field :name %><br>
  <%= f.label :user_id, "List owner (ID)" %><br>
  <%= f.text_field :user_id %><br>
  
  <%= f.submit %>

<% end %>