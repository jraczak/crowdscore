<%= render 'admin/subnav' %>

<h1>Create Venue Category</h1>

<%= form_for resource, :url => {:controller => "venue_categories", :action => "create"} do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <p>USE CAUTION UPDATING THE BELOW INFORMATION!</p><br>
  <%= f.label :factual_category_id, "Factual category ID" %><br>
  <%= f.text_field :factual_category_id %><br>
  <%= f.submit("create venue category") %>
<% end %>