<%= render partial: 'admin/subnav' %>

<%= form_for resource, :url => {:controller => "featured_venues", :action => "create"} do |f| %>

  <%= f.label "Crowdscore Venue ID" %><br>
  <%= f.text_field :venue_id %><br>
  <%= f.label :description %><br>
  <%= f.text_area :description %><br>
  <%= f.label :feature_type %><br>
  <%= f.select :feature_type, options_for_select([["standard", "standard"], ["new restaurant", "new restaurant"]], "new restaurant") %><br>
  <%= f.label "Recommending user id" %></br>
  <%= f.text_field :user_id %></br>
  <%= f.label :city %><br>
  <%= f.text_field :city %><br>
  <%= f.label :state %><br>
  <%= f.text_field :state %><br>
  <%= f.label :image_url %><br>
  <%= f.file_field :image_url %><br>
  <%= f.label :active %><br>
  <%= f.check_box :active %></br>
  
  <%= f.submit %>

<% end %>